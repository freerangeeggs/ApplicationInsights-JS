        "rollup-plugin-uglify": "^6.0.0",
        "rollup": "^0.66.0",
        "react-native": "0.59.8",
        "react": "16.8.3",
        "tslint": "^5.19.0",
        "tslint-config-prettier": "^1.18.0"
    },
    "dependencies": {
        "react-native-device-info": "^2.0.2",
        "@microsoft/applicationinsights-core-js": "^2.2.4",
